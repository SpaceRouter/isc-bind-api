;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	opengate.lan. root.opengate.lan. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.opengate.lan.
1	IN	PTR	ns.opengate.lan.
1       IN      PTR     api.opengate.lan.
